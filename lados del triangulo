#include<iostream>
using namespace std;
int main(){
float  lado1, lado2, lado3
cout<<"digita el lado 1 del triangulo";
cin>>lado1;
cout<<"digita el lado 2 del traingulo";
cin>>lado2;
cout<<"digita el lado 3 del triangulo";
cin>>lado3;
if(lado 1==lado2 and lado3==lado2 and lado1==lado3);{
cout<<"tu triangulo es equilatero";
}
else if(lado2==lado1 or lado2==lado3 or lado1==lado3);{
cout<<"tu triangulo es isoseles";
}
else if(lado 1=!lado2 and lado2=!lado3 and lado1=!lado3);{
cout<<"tu triangulo es escaleno";
else{
cout<<"Eso no es un triangulo";
}
return0;
    }
}
